    "axios": "^1.7.2",
    "clsx": "^2.1.1",
    "date-fns": "^2.30.0",
    "i18next": "^23.11.5",
    "lodash.isequal": "^4.5.0",
    "next": "^14.2.4",
    "nprogress": "^0.2.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
